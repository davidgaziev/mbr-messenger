{
	"$schema": "https://json.schemastore.org/prettierrc",
	"tabWidth": 2,
	"printWidth": 80,
	"semi": false,
	"useTabs": true,
	"singleQuote": true,
	"bracketSpacing": true,
	"bracketSameLine": false,
	"singleAttributePerLine": true,
	"arrowParens": "always",
	"trailingComma": "none",
	"quoteProps": "as-needed",
	"endOfLine": "lf",
	"plugins": [
		"prettier-plugin-tailwindcss"
	]
}
